{
  "expo": {
    "name": "mbbs-planet",
    "slug": "mbbs-planet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/initial.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.raju560.mbbsplanet"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/initial.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "09262fdf-a16b-48f6-be78-fbd547fa3957"
      }
    },
    "owner": "raju560"
  }
}
